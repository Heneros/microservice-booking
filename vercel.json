{
    "version": 2,
    "buildCommand": "npm run vercel-build",
    "builds": [
        {
            "src": "dist/apps/api-gateway/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/*(.*)",
            "dest": "dist/apps/api-gateway/main.js"
        }
    ]
}